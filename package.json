{
  "name": "leapp-aws-container",
  "author": "Luca 'remix_tj' Lorenzetto",
  "version": "0.0.2",
  "description": "Open different AWS Sessions in different firefox containers",
  "keywords": [
    "leapp-plugin",
    "AWS",
    "firefox"
  ],
  "leappPlugin": {
    "supportedOS": [
      "mac",
      "windows",
      "linux"
    ],
    "supportedSessions": [
      "awsIamRoleFederated",
      "awsIamRoleChained",
      "awsSsoRole",
      "aws"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "files": [
    "plugin.js"
  ],
  "license": "MPL",
  "dependencies": {
    "@noovolari/leapp-core": "^0.1.117"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "repository": {
   "type": "git",
   "url": "https://github.com/remixtj/leapp-container-plugin.git"
  }
}
